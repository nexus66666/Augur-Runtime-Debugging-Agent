
{
  "name": "augur-ai-debugger",
  "displayName": "Augur AI Debugger",
  "description": "An AI agent that attaches to the debug session to provide runtime analysis and control.",
  "version": "0.0.1",
  "publisher": "augur-dev",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Debuggers",
    "AI"
  ],
  "activationEvents": [
    "onDebug"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^20.11.5",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@google/genai": "^1.29.0"
  },
  "contributes": {
    "configuration": {
      "title": "Augur AI Debugger",
      "properties": {
        "augur.apiKey": {
          "type": "string",
          "default": "",
          "description": "YOUR_API_KEY"
        }
      }
    }
  }
}
